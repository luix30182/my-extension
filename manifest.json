{
	"manifest_version": 3,
	"name": "My extension",
	"version": "1.0",
	"description": "For testing chrome extension development",

	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"action": {
		"default_popup": "index.html",
		"default_icon": "icons/icon48.png"
	},
	"permissions": ["storage", "scripting", "tabs", "activeTab"],
	"background": {
		"service_worker": "service-worker.ts"
	}
}
